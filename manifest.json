{
  "name": "Political Extension",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "permissions": [
    "activeTab"
   ],
   "background": {
    "scripts": ["jquery.js", "background.js"],
    "persistent": false
   },
   "content_scripts": [
     {
       "matches": ["<all_urls>"],
       "css": ["styles.css"],
       "js": ["jquery.js", "jquery.min.js", "mark.js", "linkify.min.js", "linkify-jquery.min.js", "contentScript.js"]
     }
  ]
}
